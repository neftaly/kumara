{
  "name": "kumara",
  "version": "0.1.0",
  "description": "Streaming Signal K implementation",
  "license": "MIT",
  "repository": "https://github.com/neftaly/kumara",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "signal k",
    "signalk",
    "marine"
  ],
  "scripts": {
    "lint": "semistandard | snazzy",
    "test": "npm run lint && babel-tape-runner ./test/**/*.js | tee tap-tests.log | tap-spec",
    "build": "rm -rf dist && babel src --out-dir dist --source-maps",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "flyd": "^0.2.4",
    "immutable": "^3.8.1",
    "ramda": "^0.24.1",
    "universal-websocket-client": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "eslint": "^4.5.0",
    "semistandard": "^11.0.0",
    "snazzy": "^7.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "semistandard": {
    "parser": "babel-eslint"
  }
}
